{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "types": ["./src/types.d.ts"],
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noEmit": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "ignoreDeprecations": "5.0"
  }
}
