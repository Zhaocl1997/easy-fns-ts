{
  "name": "easy-fns-ts",
  "version": "1.0.9",
  "description": "easy-fns typescript version",
  "main": "index.js",
  "scripts": {
    "dev": "tsc -w",
    "lint": "eslint . --ext .ts -f table",
    "fix": "eslint . --ext .ts -f table --fix",
    "test": "jest",
    "cover": "jest --coverage",
    "setNPM": "npm config set registry https://registry.npmjs.org/",
    "setTB": "npm config set registry https://registry.npm.taobao.org",
    "preprod": "tsc && npm run esm",
    "prod": "npm run setNPM && npm version patch && npm publish",
    "postprod": "git push && npm run setTB",
    "esm": "tsc -m es2020 --outDir ./dist/esm"
  },
  "author": "zhaocl1997",
  "license": "ISC",
  "keywords": [
    "typescript",
    "string",
    "array",
    "regex",
    "tree",
    "time",
    "utils"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "typescript": "^4.1.2"
  }
}
