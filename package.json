{
  "name": "easy-fns-ts",
  "version": "2.0.3",
  "packageManager": "pnpm@10.12.1",
  "description": "Some useful daily used functions",
  "author": "Zhaocl1997",
  "license": "ISC",
  "homepage": "https://github.com/Zhaocl1997/easy-fns-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zhaocl1997/easy-fns-ts.git"
  },
  "bugs": "https://github.com/Zhaocl1997/easy-fns-ts/issues",
  "keywords": [
    "typescript",
    "string",
    "array",
    "regex",
    "tree",
    "time",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint 'src/**' --fix",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm lint && pnpm build",
    "publish:dry": "pnpm publish --dry-run",
    "publish:prod": "pnpm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.14.1",
    "@antfu/ni": "25.0.0",
    "@antfu/utils": "9.2.0",
    "@types/node": "22.15.30",
    "bumpp": "10.1.1",
    "eslint": "9.28.0",
    "esno": "4.8.0",
    "lint-staged": "16.1.0",
    "pnpm": "9.5.0",
    "simple-git-hooks": "2.13.0",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vite": "6.3.5",
    "vitest": "3.2.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
